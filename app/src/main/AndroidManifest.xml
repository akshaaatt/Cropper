<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aemerse.cropper.app">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:theme="@style/Theme.Sample">
        <activity
            android:name="com.aemerse.cropper.app.SMainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.aemerse.cropper.app.SCropResultActivity" />
        <activity android:name="com.aemerse.cropper.CropImageActivity" />
        <activity
            android:name="com.aemerse.cropper.app.extend_activity.app.SExtendActivity"
            android:theme="@style/Theme.SampleNoActionBar" />

    </application>
</manifest>